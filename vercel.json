{
  "version": 2,
  "builds": [
    {
      "src": "build/index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["build/**"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/build/index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": [{ "key": "Access-Control-Allow-Origin", "value": "*" }]
    }
  ]
}
